#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/-S" ]; then
  exec "$basedir/-S" node "$basedir/../@solana/errors/bin/cli.mjs" "$@"
else 
  exec -S node "$basedir/../@solana/errors/bin/cli.mjs" "$@"
fi
